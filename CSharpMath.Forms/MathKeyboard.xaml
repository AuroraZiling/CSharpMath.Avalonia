<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:r="clr-namespace:CSharpMath.Rendering;assembly=CSharpMath.Rendering"
             xmlns:i="clr-namespace:CSharpMath.Forms.MathKeyboardMarkupExtensions"
             x:Class="CSharpMath.Forms.MathKeyboard"
             WidthRequest="320"
             HeightRequest="225">
    <!-- double R(double value, double factor) => Math.Round(value / factor, MidpointRounding.AwayFromZero) * factor;
         void F(float a, float b, float c, float d) =>
           WriteLine((R(a, R(c, 2)) / 320f, R(b, R(d, 3)) / 180f, R(c, 2) / 320f, R(d, 3) / 180f).ToString().Trim('(', ')')); -->
    <ContentView.Resources>
        <r:MathKeyboard x:Key="Keyboard" x:Name="Keyboard"/>
        <Style TargetType="AbsoluteLayout">
            <Setter Property="AbsoluteLayout.LayoutFlags" Value="All"/>
        </Style>
        <Style TargetType="AbsoluteLayout" Class="Tab">
            <Setter Property="AbsoluteLayout.LayoutBounds" Value="0, 0.2, 1, 0.8"/>
            <Setter Property="IsVisible" Value="False"/>
            <Setter Property="BackgroundColor" Value="Lime"/>
        </Style>
        <Style TargetType="Button">
            <Setter Property="AbsoluteLayout.LayoutFlags" Value="All"/>
            <Setter Property="FontSize" Value="20"/>
        </Style>
        <!--Left row of first 3 tabs-->
        <Style TargetType="Button" Class="x">
            <Setter Property="Text" Value="𝑥"/>
            <Setter Property="AbsoluteLayout.LayoutBounds" Value="0, 0, 0.15625, 0.25"/>
            <Setter Property="Command" Value="{i:MathInput Keyboard={StaticResource Keyboard}, Input=SmallX}"/>
        </Style>
        <Style TargetType="Button" Class="y">
            <Setter Property="Text" Value="𝑦"/>
            <Setter Property="AbsoluteLayout.LayoutBounds" Value="0, 0.25, 0.15625, 0.25"/>
            <Setter Property="Command" Value="{i:MathInput Keyboard={StaticResource Keyboard}, Input=SmallY}"/>
        </Style>
        <Style TargetType="Button" Class="Fraction">
            <Setter Property="Text" Value="⸋⁄□"/>
            <Setter Property="AbsoluteLayout.LayoutBounds" Value="0, 0.5, 0.15625, 0.25"/>
            <Setter Property="Command" Value="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Fraction}"/>
        </Style>
        <Style TargetType="Button" Class="Power">
            <Setter Property="Text" Value="⬜⸋"/>
            <Setter Property="AbsoluteLayout.LayoutBounds" Value="0, 0.75, 0.15625, 0.25"/>
            <Setter Property="Command" Value="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Power}"/>
        </Style>
        <!--/Left row of first 3 tabs-->
        <!--Right row of first 3 tabs-->
        <Style TargetType="Button" Class="Back">
            <Setter Property="Text" Value="🔙"/>
            <Setter Property="AbsoluteLayout.LayoutBounds" Value="0.7875, 0, 0.225, 0.25"/>
            <Setter Property="Command" Value="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Backspace}"/>
        </Style>
        <Style TargetType="Button" Class="Enter">
            <Setter Property="Text" Value="⏎"/>
            <Setter Property="AbsoluteLayout.LayoutBounds" Value="0.7875, 0.25, 0.225, 0.5"/>
            <Setter Property="Command" Value="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Return}"/>
        </Style>
        <Style TargetType="Button" Class="Dismiss">
            <Setter Property="Text" Value="❌"/>
            <!--🚫-->
            <Setter Property="AbsoluteLayout.LayoutBounds" Value="0.7875, 0.75, 0.225, 0.25"/>
            <Setter Property="Command" Value="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Dismiss}"/>
        </Style>
        <!--/Right row of first 3 tabs-->
    </ContentView.Resources>
    <AbsoluteLayout BackgroundColor="Aqua">
        <Button Text="◄" AbsoluteLayout.LayoutBounds="0, 0, 0.125, 0.2" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Left}"/>
        <Button x:Name="NumbersButton" Text="123" AbsoluteLayout.LayoutBounds="0.125, 0, 0.1875, 0.25" Command="{x:Binding SwitchTab}" CommandParameter="{Reference Numbers}"/>
        <AbsoluteLayout x:Name="Numbers" StyleClass="Tab" IsVisible="True">
            <Button StyleClass="x"/>
            <Button StyleClass="y"/>
            <Button StyleClass="Fraction"/>
            <Button StyleClass="Power"/>
            <Button Text="7" AbsoluteLayout.LayoutBounds="0.15625, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D7}"/>
            <Button Text="8" AbsoluteLayout.LayoutBounds="0.3125, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D8}"/>
            <Button Text="9" AbsoluteLayout.LayoutBounds="0.46875, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D9}"/>
            <Button Text="÷" AbsoluteLayout.LayoutBounds="0.625, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Divide}"/>
            <Button Text="4" AbsoluteLayout.LayoutBounds="0.15625, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D4}"/>
            <Button Text="5" AbsoluteLayout.LayoutBounds="0.3125, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D5}"/>
            <Button Text="6" AbsoluteLayout.LayoutBounds="0.46875, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D6}"/>
            <Button Text="×" AbsoluteLayout.LayoutBounds="0.625, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Multiply}"/>
            <Button Text="1" AbsoluteLayout.LayoutBounds="0.15625, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D1}"/>
            <Button Text="2" AbsoluteLayout.LayoutBounds="0.3125, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D2}"/>
            <Button Text="3" AbsoluteLayout.LayoutBounds="0.46875, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D3}"/>
            <Button Text="−" AbsoluteLayout.LayoutBounds="0.625, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Minus}"/>
            <Button Text="0" AbsoluteLayout.LayoutBounds="0.15625, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=D0}"/>
            <Button Text="." AbsoluteLayout.LayoutBounds="0.3125, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Decimal}"/>
            <Button Text="=" AbsoluteLayout.LayoutBounds="0.46875, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Equals}"/>
            <Button Text="+" AbsoluteLayout.LayoutBounds="0.625, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Plus}"/>
            <Button StyleClass="Back"/>
            <Button StyleClass="Dismiss"/>
            <Button StyleClass="Enter"/>
        </AbsoluteLayout>
        <Button x:Name="OperationsButton" Text="()&lt;&gt;" AbsoluteLayout.LayoutBounds="0.3125, 0, 0.1875, 0.25" Command="{x:Binding SwitchTab}" CommandParameter="{Reference Operations}"/>
        <AbsoluteLayout x:Name="Operations" StyleClass="Tab">
            <Button StyleClass="x"/>
            <Button StyleClass="y"/>
            <Button StyleClass="Fraction"/>
            <Button StyleClass="Power"/>
            <Button Text="(" AbsoluteLayout.LayoutBounds="0.15625, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=LeftRoundBracket}"/>
            <Button Text=")" AbsoluteLayout.LayoutBounds="0.3125, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=RightRoundBracket}"/>
            <Button Text="&lt;" AbsoluteLayout.LayoutBounds="0.46875, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=LessThan}"/>
            <Button Text="&gt;" AbsoluteLayout.LayoutBounds="0.625, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=GreaterThan}"/>
            <Button Text="[" AbsoluteLayout.LayoutBounds="0.15625, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=LeftSquareBracket}"/>
            <Button Text="]" AbsoluteLayout.LayoutBounds="0.3125, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=RightSquareBracket}"/>
            <Button Text="≤" AbsoluteLayout.LayoutBounds="0.46875, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=LessOrEquals}"/>
            <Button Text="≥" AbsoluteLayout.LayoutBounds="0.625, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=GreaterOrEquals}"/>
            <Button Text="{}{" AbsoluteLayout.LayoutBounds="0.15625, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=LeftCurlyBracket}"/>
            <Button Text="}" AbsoluteLayout.LayoutBounds="0.3125, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=RightCurlyBracket}"/>
            <Button Text="|⬜|" AbsoluteLayout.LayoutBounds="0.46875, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Absolute}"/>
            <Button Text="%" AbsoluteLayout.LayoutBounds="0.625, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Percentage}"/>
            <Button Text="!" AbsoluteLayout.LayoutBounds="0.15625, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Factorial}"/>
            <Button Text="∞" AbsoluteLayout.LayoutBounds="0.3125, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Infinity}"/>
            <Button Text="∶" AbsoluteLayout.LayoutBounds="0.46875, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Ratio}"/>
            <Button Text="," AbsoluteLayout.LayoutBounds="0.625, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Comma}"/>
            <Button StyleClass="Back"/>
            <Button StyleClass="Dismiss"/>
            <Button StyleClass="Enter"/>
        </AbsoluteLayout>
        <Button x:Name="FunctionsButton" Text="f(x)" AbsoluteLayout.LayoutBounds="0.5, 0, 0.1875, 0.25" Command="{x:Binding SwitchTab}" CommandParameter="{Reference Functions}"/>
        <AbsoluteLayout x:Name="Functions" StyleClass="Tab">
            <Button StyleClass="x"/>
            <Button StyleClass="y"/>
            <Button StyleClass="Fraction"/>
            <Button StyleClass="Power"/>
            <Button Text="sin" AbsoluteLayout.LayoutBounds="0.15625, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Sine}"/>
            <Button Text="cos" AbsoluteLayout.LayoutBounds="0.3125, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Cosine}"/>
            <Button Text="tan" AbsoluteLayout.LayoutBounds="0.46875, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Tangent}"/>
            <Button Text="θ" AbsoluteLayout.LayoutBounds="0.625, 0, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=SmallTheta}"/>
            <Button Text="sec" AbsoluteLayout.LayoutBounds="0.15625, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Secant}"/>
            <Button Text="csc" AbsoluteLayout.LayoutBounds="0.3125, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Cosecant}"/>
            <Button Text="cot" AbsoluteLayout.LayoutBounds="0.46875, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Cotangent}"/>
            <Button Text="π" AbsoluteLayout.LayoutBounds="0.625, 0.25, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=SmallPi}"/>
            <Button Text="log" AbsoluteLayout.LayoutBounds="0.15625, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Logarithm}"/>
            <Button Text="ln" AbsoluteLayout.LayoutBounds="0.3125, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=NaturalLogarithm}"/>
            <Button Text="log▖" AbsoluteLayout.LayoutBounds="0.46875, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=LogarithmWithBase}"/>
            <Button Text="∠" AbsoluteLayout.LayoutBounds="0.625, 0.5, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Angle}"/>
            <Button Text="⬜▖" AbsoluteLayout.LayoutBounds="0.15625, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Subscript}"/>
            <Button Text="√⬜" AbsoluteLayout.LayoutBounds="0.3125, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=SquareRoot}"/>
            <Button Text="⸋√⬜" AbsoluteLayout.LayoutBounds="0.46875, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=NthRoot}"/>
            <Button Text="°" AbsoluteLayout.LayoutBounds="0.625, 0.75, 0.15625, 0.25" Command="{i:MathInput Keyboard={StaticResource Keyboard}, Input=Degree}"/>
            <Button StyleClass="Back"/>
            <Button StyleClass="Dismiss"/>
            <Button StyleClass="Enter"/>
        </AbsoluteLayout>
